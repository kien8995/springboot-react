{
  "name": "spring-react",
  "version": "1.0.0",
  "description": "Starter kit for Spring React and Redux",
  "scripts": {
    "prestart": "babel-node tools/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "lint": "node_modules/.bin/esw webpack.config.* src/main/webapp tools",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter spec tools/testSetup.js \"src/main/webapp/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "remove-dist": "node_modules/.bin/rimraf src/main/resources/static/dist/*",
    "clean-dist": "npm run remove-dist && mkdirp src/main/resources/static/dist",
    "build:html": "babel-node tools/buildHtml.js",
    "prebuild": "npm-run-all clean-dist lint build:html",
    "build": "babel-node tools/build.js",
    "postbuild": "babel-node tools/distServer.js"
  },
  "author": "kientran",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.13.1",
    "babel-polyfill": "6.13.0",
    "bootstrap": "3.3.7",
    "classnames": "^2.2.3",
    "ejs": "^2.5.1",
    "es6-promise": "^3.2.1",
    "jquery": "3.1.0",
    "js-base64": "^2.1.9",
    "react": "^15.0.2",
    "react-addons-pure-render-mixin": "^15.3.1",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "react-router-redux": "^4.0.4",
    "redux": "^3.5.2",
    "redux-thunk": "^2.0.1",
    "toastr": "2.1.2"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "postcss-color-rebeccapurple": "2.0.0",
    "babel-cli": "6.11.4",
    "babel-core": "6.13.2",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-preset-es2015": "6.13.2",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-register": "6.11.6",
    "colors": "1.1.2",
    "compression": "1.6.2",
    "cross-env": "2.0.0",
    "css-loader": "0.23.1",
    "css-modules-require-hook": "^4.0.0",
    "postcss-loader": "^0.10.1",
    "postcss-modules-extract-imports": "^1.0.0",
    "postcss-modules-local-by-default": "^1.0.0",
    "postcss-modules-scope": "^1.0.0",
    "postcss-modules-values": "^1.1.1",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "cssnano": "^3.7.4",
    "enzyme": "2.4.1",
    "eslint": "3.3.1",
    "eslint-plugin-import": "1.13.0",
    "eslint-plugin-react": "6.1.2",
    "eslint-watch": "2.1.14",
    "eventsource-polyfill": "0.9.6",
    "expect": "1.20.2",
    "express": "4.14.0",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "jsdom": "9.4.2",
    "mocha": "3.0.2",
    "nock": "8.0.0",
    "npm-run-all": "3.0.0",
    "open": "0.0.5",
    "react-addons-test-utils": "15.3.1",
    "redux-immutable-state-invariant": "1.2.3",
    "redux-mock-store": "1.1.4",
    "rimraf": "2.5.4",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kien8995/springboot-react"
  }
}
